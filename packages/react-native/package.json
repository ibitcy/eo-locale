{
  "name": "@eo-locale/react-native",
  "version": "1.6.0-beta.0",
  "description": "Easy way to localize your React Native application",
  "author": "Maksim Sharipov <m.v.sharipov@gmail.com>",
  "homepage": "https://eo-locale.netlify.app",
  "license": "ISC",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "type": "module",
  "exports": {
    "import": "./lib/index.js",
    "require": "./lib/index.cjs"
  },
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">= 12"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup --config config/rollup.config.js",
    "postbuild": "copyup \"src/**/*.flow\" lib && size-limit",
    "prepare": "npm run build"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibitcy/eo-locale.git"
  },
  "bugs": {
    "url": "https://github.com/ibitcy/eo-locale/issues"
  },
  "keywords": [
    "internationalization",
    "intl",
    "messageformat",
    "plural",
    "icu",
    "react",
    "tiny",
    "i18n"
  ],
  "size-limit": [
    {
      "path": "lib/index.cjs",
      "limit": "700B",
      "webpack": false
    }
  ],
  "dependencies": {
    "@eo-locale/core": "workspace:^"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/jest": "^27.4.1",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "copyfiles": "^2.4.1",
    "flow-bin": "^0.176.3",
    "jest": "^28.0.2",
    "react": "^18.1.0",
    "rollup": "^2.70.2",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "size-limit": "^7.0.8",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.2"
  },
  "gitHead": "85f16888e69bd8962e3e20ad92065a6ef5afcb22"
}
