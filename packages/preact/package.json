{
  "name": "@eo-locale/preact",
  "version": "1.7.2",
  "description": "Easy way to localize your Preact application",
  "homepage": "https://eo-locale.netlify.app",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "type": "module",
  "exports": {
    "import": "./lib/index.js",
    "require": "./lib/index.cjs"
  },
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">= 12"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "yarn run --top-level tsup src/index.tsx --format cjs,esm --dts --clean --sourcemap --out-dir lib --minify",
    "postbuild": "size-limit",
    "preversion": "npm run build"
  },
  "size-limit": [
    {
      "path": "lib/index.cjs",
      "limit": "2.5kB"
    }
  ],
  "keywords": [
    "internationalization",
    "intl",
    "messageformat",
    "plural",
    "icu",
    "preact",
    "tiny",
    "i18n"
  ],
  "author": {
    "name": "Maksim Sharipov",
    "url": "https://github.com/pret-a-porter"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibitcy/eo-locale.git"
  },
  "bugs": {
    "url": "https://github.com/ibitcy/eo-locale/issues"
  },
  "license": "ISC",
  "devDependencies": {
    "preact": "^10.24.3"
  },
  "peerDependencies": {
    "preact": "^10.1.1"
  },
  "dependencies": {
    "@eo-locale/core": "workspace:*"
  },
  "publishConfig": {
    "workspaceProtocol": "replace"
  }
}
